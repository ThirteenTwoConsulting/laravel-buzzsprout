{
    "name": "thirteentwoconsulting/laravel-buzzsprout",
    "version": "0.0.1",
    "description": "Buzzsprout API integration for Laravel 10+",
    "require": {
        "illuminate/http": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.12",
        "pestphp/pest-plugin-laravel": "^2.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ThirteenTwo\\LaravelBuzzsprout\\": "./src/"
        }
    },
    "authors": [
        {
            "name": "Eric Hansen",
            "email": "eric@thirteentwo.co"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": []
        }
    },
    "stability": "stable"
}
